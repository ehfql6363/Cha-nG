{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react(.*)",
    "<THIRD_PARTY_MODULES>",
    "^@tanstack/(.*)$",
    "^styled-components",
    "^@/(.*)$",
    "^@app/(.*)$",
    "^@common/(.*)$",
    "^@components/(.*)$",
    "^@apis/(.*)$",
    "^@assets/(.*)$",
    "^@hooks/(.*)$",
    "^@stores/(.*)$",
    "^@utils/(.*)$",
    "^@types/(.*)$",
    "^@styles/(.*)$",
    "^@mocks/(.*)$",
    "^@constants/(.*)$",
    "^App",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "endOfLine": "lf",
  "printWidth": 80,
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "useTabs": false,
  "singleAttributePerLine": true,
  "jsxSingleQuote": false,
  "trailingComma": "all",
  "bracketSameLine": true,
  "arrowParens": "always"
}
